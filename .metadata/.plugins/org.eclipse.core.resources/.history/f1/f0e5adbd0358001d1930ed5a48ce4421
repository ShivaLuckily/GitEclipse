package flipkart.com;

public class Automation {
	
	public static void main(String[] args) {
		
		
		String s="i like automation testing";
		
		String[] s1=s.split(" ");
		
	
		
		for(String word:s1) {
			
			if("automatio".equals(word)) {
				
				
				System.out.println(word);
				
				
			}
			
		}
		
	}

}
